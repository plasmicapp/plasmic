{
  "name": "@plasmicpkgs/plasmic-strapi",
  "version": "0.1.77",
  "description": "Plasmic Strapi components.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/plasmic-strapi.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "yarn --cwd=../.. test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/plasmic-strapi.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/plasmic-strapi.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@plasmicapp/host": "1.0.132",
    "@plasmicapp/query": "0.1.63",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/dlv": "^1.1.2",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "husky": "^7.0.4",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@types/dlv": "^1.1.2",
    "change-case": "^4.1.2",
    "dlv": "^1.1.3"
  }
}
