{
  "name": "@plasmicpkgs/plasmic-query",
  "version": "0.0.227",
  "description": "Plasmic components and registration calls for data fetching",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/plasmic-query.esm.js",
  "files": [
    "dist"
  ],
  "size-limit": [
