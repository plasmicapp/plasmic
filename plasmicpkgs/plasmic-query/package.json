    "@types/node": "^14.0.26",
    "@types/react": "^18.2.33",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0"
  },
  "peerDependencies": {
    "@plasmicapp/host": ">=1.0.0",
    "@plasmicapp/query": ">=0.1.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
