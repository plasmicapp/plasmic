    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0"
  },
  "peerDependencies": {
    "@plasmicapp/host": ">=1.0.0",
