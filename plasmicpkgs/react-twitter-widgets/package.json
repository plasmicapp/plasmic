{
  "name": "@plasmicpkgs/react-twitter-widgets",
  "version": "0.0.143",
  "description": "Plasmic registration calls for react-twitter-widgets",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/react-twitter-widgets.esm.js",
  "files": [
    "dist"
  ],
  "size-limit": [
