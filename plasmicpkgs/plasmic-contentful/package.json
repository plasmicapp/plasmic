{
  "name": "@plasmicpkgs/plasmic-contentful",
  "version": "0.0.171",
  "description": "Plasmic Contentful components.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node src/index.tsx --dts --format esm,cjs --target es2019",
    "test": "TEST_CWD=`pwd` yarn --cwd=../.. test --passWithNoTests",
    "prepublishOnly": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@plasmicapp/host": "1.0.227",
    "@plasmicapp/query": "0.1.81",
    "@types/dlv": "^1.1.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "husky": "^7.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.3.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.13.1",
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "change-case": "^4.1.2",
    "dlv": "^1.1.3"
  }
}
