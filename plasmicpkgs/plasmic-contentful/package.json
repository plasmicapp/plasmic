{
  "name": "@plasmicpkgs/plasmic-contentful",
  "version": "0.0.179",
  "description": "Plasmic Contentful components.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plasmicapp/plasmic.git",
    "directory": "plasmicpkgs/plasmic-contentful"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:types && yarn build:index",
    "build:types": "yarn tsc",
    "build:index": "node ../../build.mjs ./src/index.tsx",
    "test": "TEST_CWD=`pwd` yarn --cwd=../.. test --passWithNoTests",
    "prepublishOnly": "npm run build",
    "postpublish": "bash ../../scripts/publish-api-doc-model.sh"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@plasmicapp/host": "1.0.231",
    "@plasmicapp/query": "0.1.81",
    "@plasmicpkgs/contentful": "0.0.4",
    "@types/dlv": "^1.1.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.13.1",
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "change-case": "^4.1.2",
    "dlv": "^1.1.3"
  },
  "peerDependencies": {
    "@plasmicapp/host": "^1.0.211",
    "react": ">=16.8.0"
  }
}
