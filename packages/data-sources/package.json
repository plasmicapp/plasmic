{
  "version": "0.1.24",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@plasmicapp/data-sources",
  "author": "Chung Wu",
  "module": "dist/data-sources.esm.js",
  "size-limit": [
    {
      "path": "dist/data-sources.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/data-sources.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@plasmicapp/data-sources-context": "0.1.4",
    "@plasmicapp/host": "1.0.94",
    "@plasmicapp/isomorphic-unfetch": "^1.0.1",
    "@plasmicapp/query": "0.1.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
