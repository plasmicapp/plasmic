{
  "version": "1.0.69",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "yalcp": "yalc publish --push",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@plasmicapp/watcher",
  "author": "Chung Wu",
  "module": "dist/watcher.esm.js",
  "size-limit": [
    {
      "path": "dist/watcher.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/watcher.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "husky": "^6.0.0",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "socket.io-client": "^4.1.2"
  },
  "gitHead": "fa53f7d79f0e26d8b061102fda0c06788da6f8a7"
}
