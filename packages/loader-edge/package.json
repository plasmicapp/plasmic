{
  "version": "1.0.10",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "yalcp": "yalc publish --push",
    "test": "tsdx test --passWithNoTests",
    "coverage": "tsdx test --coverage --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build --target node",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@plasmicapp/loader-edge",
  "author": "Chung Wu",
  "module": "dist/loader-edge.esm.js",
  "size-limit": [
    {
      "path": "dist/loader-edge.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/loader-edge.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "husky": "^6.0.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@plasmicapp/loader-fetcher": "1.0.4",
    "@plasmicapp/loader-splits": "1.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
